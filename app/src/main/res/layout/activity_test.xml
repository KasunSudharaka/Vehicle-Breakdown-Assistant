<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TestActivity"
    android:orientation="vertical"
    android:background="@drawable/g5"
    >


  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="500dp"
      android:layout_marginBottom="0dp"
      android:layout_gravity="bottom"
      android:background="@color/colorAccentnew"
      android:id="@+id/ratingpanel"
      android:orientation="vertical"
      android:visibility="visible">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Service Summary"
        android:textStyle="bold"
        android:textSize="30dp"
        android:textColor="@color/colorPrimarynew"
        android:layout_marginTop="20dp"
        />



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="20dp">

      <RatingBar
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:id="@+id/rfive"
          android:numStars="5"
          android:stepSize="0.1"
          />

      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="20dp"
          android:id="@+id/fivet"
          android:textSize="20dp"
          android:textColor="#fff"
          android:text="0"
          android:layout_gravity="center"/>

      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="15dp"
          android:text="ratings"
          android:layout_gravity="center"
          android:textSize="15dp"
          android:textColor="#fff"/>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

      <RatingBar
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:id="@+id/rfour"
          android:numStars="5"
          android:stepSize="0.1"
          />

      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="20dp"
          android:id="@+id/fourt"
          android:textSize="20dp"
          android:textColor="#fff"
          android:text="0"
          android:layout_gravity="center"/>

      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="15dp"
          android:text="ratings"
          android:layout_gravity="center"
          android:textSize="15dp"
          android:textColor="#fff"/>


    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

      <RatingBar
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:id="@+id/rthree"
          android:numStars="5"
          android:stepSize="0.1"
          />

      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="20dp"
          android:id="@+id/threet"
          android:textSize="20dp"
          android:textColor="#fff"
          android:text="0"
          android:layout_gravity="center"/>

      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="15dp"
          android:text="ratings"
          android:layout_gravity="center"
          android:textSize="15dp"
          android:textColor="#fff"/>


    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

      <RatingBar
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:id="@+id/rtwo"
          android:numStars="5"
          android:stepSize="0.1"
          />

      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="20dp"
          android:id="@+id/twot"
          android:textSize="20dp"
          android:textColor="#fff"
          android:text="0"
          android:layout_gravity="center"/>

      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="15dp"
          android:text="ratings"
          android:layout_gravity="center"
          android:textSize="15dp"
          android:textColor="#fff"/>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

      <RatingBar
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:id="@+id/rone"
          android:numStars="5"
          android:stepSize="0.1"
          />

      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="20dp"
          android:id="@+id/onet"
          android:textSize="20dp"
          android:textColor="#fff"
          android:text="0"
          android:layout_gravity="center"/>

      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="15dp"
          android:text="ratings"
          android:layout_gravity="center"
          android:textSize="15dp"
          android:textColor="#fff"/>


    </LinearLayout>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Averge Rating "
        android:layout_gravity="center"
        android:textSize="20dp"
        android:textStyle="italic"
        android:textColor="#fff"/>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center"
        >
    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/raverage"
        android:numStars="5"
        android:stepSize="0.1"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:text="0"
        android:id="@+id/averaget"
        android:layout_gravity="center"
        android:textSize="20dp"
        android:textColor="#fff"/>

    </LinearLayout>

    <!--<Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end"
        android:text="Rate Later"
        android:textStyle="bold"
        android:background="@color/colorAccent"
        android:layout_marginEnd="8sp"
        android:layout_marginTop="8sp"
        android:id="@+id/laterbtn"/>-->

  </LinearLayout>




  <TextView
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="center"
      android:textStyle="italic"
      android:textSize="25dp"
      android:text="Welldone! \n You have served for"
      android:textAlignment="center"
      android:textColor="#fff"
      android:layout_marginTop="10dp"
      />

  <TextView
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="center"
      android:textStyle="bold"
      android:textSize="35dp"
      android:text="0"
      android:textColor="#fff"
      android:layout_marginTop="10dp"
      android:id="@+id/testcount"/>


  <TextView
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="center"
      android:textStyle="italic"
      android:textSize="25dp"
      android:text="Customers :)"
      android:textAlignment="center"
      android:textColor="#fff"
      android:layout_marginTop="10dp"
      />



</LinearLayout>